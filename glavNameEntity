public abstract class NameEntity {

    private String name;

    public NameEntity(String name) {
        this.name = name;
    }

    public NameEntity() {
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
       return this.getClass().getSimpleName()+"\nName:"+name;
    }
}
