public  static void arrayMin_Max()
    {
            int [] array =new int []{4,2,3,51,6,8};
            int max=array[0];
            int min=array[0];

            for (int i=0;i<array.length;i++)
            {
                if(max<array[i])
                    max=array[i];
                if(min>array[i])
                    min=array[i];
            }
        System.out.println("Max="+max);
        System.out.println("Min="+min);
        
        }
